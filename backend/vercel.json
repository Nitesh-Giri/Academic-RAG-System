{
    "version": 2,
    "functions": {
      "index.js": {
        "memory": 1024,
        "maxDuration": 60
      }
    },
    "routes": [
      { "src": "/(.*)", "dest": "index.js" }
    ],
    "env": {
      "MONGODB_URI": "@mongodb_uri",
      "GEMINI_API_KEY": "@gemini_api_key",
      "NODE_ENV": "production"
    }
  }
  